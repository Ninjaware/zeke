#!/bin/sh
echo
echo
echo "         Zeke booting"

# TODO read /etc/rc.conf, fsck, set hostname etc

# Since the rootfs is FAT we need to do chmod +x for /bin before users can login
cd /bin
ls | xargs chmod +x
cd /

# TODO Move to fstab when implemented
mount -t fatfs /dev/emmc0p0 /boot

exec /sbin/getty
